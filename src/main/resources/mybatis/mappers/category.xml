<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="category">

	<insert id="adminCategoryAdd" parameterType="map">
		<![CDATA[
			insert into category values (null, #{categoryname}, #{categorydesc}, now(), #{userid})
		]]>
	</insert>
	
	<select id="getAllCategoryList"  resultType="categoryvo">
		<![CDATA[
			select category_no as categoryNo, category_name as categoryName, 
			category_desc as categoryDesc, category_regdate as categoryRegdate, 
			user_id as userId 
			from category
		]]>
	</select>

</mapper>